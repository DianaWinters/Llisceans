<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>Lliscean</defName>
    <label>lliscean</label>
    <description>Created due to a fad on a distant glitterworld, the Lliscean is a race of half-human half-deer people. Friendly and nature loving, those that get stranded on rimworlds generally settle into idyllic (at least for a time) tribal societies. The race is mostly female, with the few males appearing feminine as well.</description>
    <alienRace>
      <generalSettings>
        <maleGenderProbability>33</maleGenderProbability>
        <immuneToAge>false</immuneToAge>
        <forcedRaceTraitEntries>
          <li>
            <defName>Beauty</defName>
            <degree>2</degree>
            <chance>25</chance>
          </li>
		  <li>
			<defName>Nudist</defName>
			<chance>25></chance>
		  </li>
        </forcedRaceTraitEntries>
        <disallowedTraits>
          <li>Ugly</li>
          <li>Pyromaniac</li>
        </disallowedTraits>
        <maxDamageForSocialfight>8</maxDamageForSocialfight>
        <humanRecipeImport>true</humanRecipeImport>

        <alienPartGenerator>
          <aliencrowntypes>
            <li>Average_Normal</li>
            <li>Average_Pointy</li>
            <li>Average_Wide</li>
            <li>Narrow_Normal</li>
            <li>Narrow_Pointy</li>
          </aliencrowntypes>
          <alienbodytypes>
            <li>Female</li>
			<li>Thin</li>
          </alienbodytypes>
          <alienskincolorgen Class="AlienRace.ColorGenerator_SkinColorMelanin">
            <minMelanin>0</minMelanin>
            <maxMelanin>1</maxMelanin>
          </alienskincolorgen>

          <alienskinsecondcolorgen Class="ColorGenerator_Options"> <!-- Borrowed from Arachne, change these at some point... keep to mostly "deer" colors, but maybe add some pastel colors as well to allude to their glitterworld origins -->
            <options>
              <!-- == Pink == -->
              <li>
                <weight>10</weight>
                <only>RGBA(1,0.6,0.8,1)</only>
              </li>
              <!-- == Blue == -->
              <li>
                <weight>10</weight>
                <only>RGBA(0.3,0.6,1,1)</only>
              </li>
              <!-- == Purple == -->
              <li>
                <weight>10</weight>
                <only>RGBA(0.5,0.5,1,1)</only>
              </li>
              <!-- == Orange == -->
              <li>
                <weight>10</weight>
                <only>RGBA(1,0.8,0.3,1)</only>
              </li>
              <!-- == Black == -->
              <li>
                <weight>10</weight>
                <only>RGBA(0.5,0.5,0.5,1)</only>
              </li>
              <!-- == Green == -->
              <li>
                <weight>10</weight>
                <only>RGBA(0.5,0.8,0.5,1)</only>
              </li>
            </options>
          </alienskinsecondcolorgen>
          <customDrawSize>(0.9, 0.9)</customDrawSize>
          <customHeadDrawSize>(0.9, 0.9)</customHeadDrawSize>
          <customPortraitDrawSize>(1.0, 1.0)</customPortraitDrawSize>
          <customPortraitHeadDrawSize>(1.0, 1.0)</customPortraitHeadDrawSize>
          <headOffset>(0.0, 0.0)</headOffset>

          <bodyAddons>
            <li>
              <drawSize>0.9</drawSize>
              <path>Things/Pawn/Lliscean/Body/LlisceanBody</path>
              <bodyPart>Waist</bodyPart>
              <inFrontOfBody>false</inFrontOfBody>
              <useSkinColor>true</useSkinColor>
              <layerOffset>-0.270</layerOffset>
              <offsets>
                <south>
                  <bodyTypes>
                    <Female>(0,0)</Female>
                  </bodyTypes>
                </south>
                <north>
                  <bodyTypes>
                    <Female>(0,0.321)</Female>
                  </bodyTypes>
                </north>
                <east>
                  <bodyTypes>
                    <Female>(-0.366,0.045)</Female>
                  </bodyTypes>
                </east>
              </offsets>
              <drawnInBed>false</drawnInBed>
            </li>
			<li> <!-- Need to do all the offsets for this; don't expect them to be remotely close yet -->
              <drawSize>0.9</drawSize>
              <path>Things/Pawn/Lliscean/Body/LlTail</path>
              <bodyPart>Tail</bodyPart>
              <inFrontOfBody>false</inFrontOfBody>
              <useSkinColor>true</useSkinColor>
              <layerOffset>-0.270</layerOffset>
              <offsets>
                <south>
                  <bodyTypes>
                    <Female>(0,0)</Female>
                  </bodyTypes>
                </south>
                <north>
                  <bodyTypes>
                    <Female>(0,0.321)</Female>
                  </bodyTypes>
                </north>
                <east>
                  <bodyTypes>
                    <Female>(-0.366,0.045)</Female>
                  </bodyTypes>
                </east>
              </offsets>
              <drawnInBed>false</drawnInBed>
            </li>
          </bodyAddons>

        </alienPartGenerator>

      </generalSettings>

      <graphicPaths>
        <li>
          <head>Things/Pawn/Arachne/Head/</head>
          <body>Things/Pawn/Arachne/Body/</body>
          <skeleton>Things/Pawn/Humanlike/Bodies/Dessicated/Dessicated_Thin</skeleton>
        </li>
      </graphicPaths>

      <hairSettings>
        <hasHair>true</hasHair>
        <getsGreyAt>50</getsGreyAt>
        <hairTags>
          <li>Rural</li>
          <li>Tribal</li>
        </hairTags>
      </hairSettings>

      <thoughtSettings>
        <cannotReceiveThoughts>
		  <li>SleptOnGround</li>
          <li>Naked</li>
        </cannotReceiveThoughts>
      </thoughtSettings>

    </alienRace>
    <statBases>
      <Mass>80</Mass>
      <MarketValue>1750</MarketValue>
      <MoveSpeed>5.5</MoveSpeed>
      <ComfyTemperatureMin>10</ComfyTemperatureMin>
      <ComfyTemperatureMax>36</ComfyTemperatureMax>
      <LeatherAmount>50</LeatherAmount>
      <PlantWorkSpeed>1.3</PlantWorkSpeed>
      <PlantHarvestYield>1.2</PlantHarvestYield>
      <SocialImpact>1.2</SocialImpact>
      <NegotiationAbility>1.1</NegotiationAbility>
      <TradePriceImprovement>1.1</TradePriceImprovement>
      <TameAnimalChance>1.3</TameAnimalChance>
      <TrainAnimalChance>1.3</TrainAnimalChance>
      <AnimalGatherSpeed>1.3</AnimalGatherSpeed>
    </statBases>
    <tools>

      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.2</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>22</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.2</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>22</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
            <li>
        <label>left hoof</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>7</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftLeg</linkedBodyPartsGroup>
      </li>
      <li>
        <label>right hoof</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>7</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>FrontRightLeg</linkedBodyPartsGroup>
      </li>
      <li>
        <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>8</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.5</chanceFactor>
      </li>
    </tools>
    <race>
      <thinkTreeMain>Humanlike</thinkTreeMain>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <intelligence>Humanlike</intelligence>
      <makesFootprints>true</makesFootprints>
      <lifeExpectancy>80</lifeExpectancy>
      <leatherDef>Leather_Lliscean</leatherDef>
      <nameCategory>HumanStandard</nameCategory>
      <hasGenders>true</hasGenders>
      <body>Lliscean_body</body>
      <baseBodySize>0.9</baseBodySize>
      <baseHealthScale>1</baseHealthScale>
      <baseHungerRate>1</baseHungerRate>
      <foodType>OmnivoreHuman</foodType>
      <nuzzleMtbHours>3</nuzzleMtbHours>
      <gestationPeriodDays>45</gestationPeriodDays>
      <meatMarketValue>0.8</meatMarketValue>
      <manhunterOnDamageChance>0</manhunterOnDamageChance>
      <manhunterOnTameFailChance>0</manhunterOnTameFailChance>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(1.01, 0.02)</li>
          <li>(3.5, 0)</li>
        </points>
      </litterSizeCurve>
      <lifeStageAges>
        <li>
          <def>HumanlikeBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>HumanlikeToddler</def>
          <minAge>1.2</minAge>
        </li>
        <li>
          <def>HumanlikeChild</def>
          <minAge>4</minAge>
        </li>
        <li>
          <def>HumanlikeTeenager</def>
          <minAge>13</minAge>
        </li>
        <li>
          <def>HumanlikeAdult</def>
          <minAge>18</minAge>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
      <specialShadowData>
        <volume>(0.3, 0.8, 0.4)</volume>
        <offset>(0,0,-0.3)</offset>
      </specialShadowData>
      <ageGenerationCurve>
        <points>
          <li>(16,0)</li>
          <li>(20,100)</li>
          <li>(40,100)</li>
          <li>(60,50)</li>
          <li>(70,30)</li>
          <li>(80,20)</li>
          <li>(120,10)</li>
          <li>(180,1)</li>
          <li>(250,0)</li>
        </points>
      </ageGenerationCurve>
      <hediffGiverSets>
        <li>OrganicStandard</li>
        <li>Human</li>
      </hediffGiverSets>
    </race>
    <!--          Recipe          -->
    <recipes>
      <li>ExciseCarcinoma</li>
      <li>AdministerMechSerumHealer</li>
      <li>RemoveBodyPart</li>
      <li>Euthanize</li>
      <li>Anesthetize</li>
    </recipes>
  </AlienRace.ThingDef_AlienRace>

</Defs>